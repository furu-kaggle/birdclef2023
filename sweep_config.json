{
    "project":"birdclef2023",
    "name": "opt_nmel128_ecanfnet_100fpers",
    "method": "bayes",
    "metric": {
        "name": "mean_score",
        "goal": "maximize"
    },
    "early_terminate":{
        "type": "hyperband",
        "min_iter": 15
    },
    "parameters":{
        "sec_labelp":{
            "distribution": "uniform",
            "min": 0.4,
            "max": 0.7
        },
        "mixup_out_prob":{
            "distribution": "uniform",
            "min": 0.1,
            "max": 0.3
        },
        "batch_size":{
            "values":[8,10,12]
        },
        "lr":{
            "distribution": "uniform",
            "min": 3e-4,
            "max": 1e-2
        },
        "lr_min":{
            "distribution": "uniform",
            "min": 1e-6,
            "max": 3e-5
        },
        "lr_ratio":{
            "distribution":"int_uniform",
            "min":3,
            "max":15
        },
        "smooth":{
            "distribution": "uniform",
            "min": 0.001,
            "max": 0.01
        },
        "k_decay":{
            "distribution": "uniform",
            "min": 1.0,
            "max": 2.5
        }
    }
}